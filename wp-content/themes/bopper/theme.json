					"name": "M"
				},
				{
					"fluid": {
						"min": "1.375rem",
						"max": "1.5rem"
					},
					"slug": "large",
					"size": "1.375rem",
					"name": "L"
				},
				{
					"fluid": {
						"min": "1.75rem",
						"max": "2.5rem"
					},
					"slug": "x-large",
					"size": "2.5rem",
					"name": "XL"
				},
				{
					"fluid": {
						"min": "2.25rem",
						"max": "4.625rem"
					},
					"slug": "xx-large",
					"size": "4.625rem",
					"name": "XXL"
				},
				{
					"fluid": {
						"min": "2.5rem",
						"max": "6.25rem"
					},
					"slug": "xxx-large",
					"size": "6.25rem",
					"name": "3XL"
				}
			]
		},
		"custom": {
			"spacing": {
				"small": "max(1.5rem, 2vw)",
				"medium": "clamp(2rem, 8vw, calc(4 * 1.5rem))",
				"large": "clamp(4rem, 10vw, 8rem)",
				"outer": "var(--wp--custom--spacing--small, 1.5rem)"
			},
			"typography": {
				"font-size": {
					"huge": "clamp(2.25rem, 4vw, 2.75rem)",
					"gigantic": "clamp(2.75rem, 6vw, 3.25rem)",
					"colossal": "clamp(3.25rem, 8vw, 6.25rem)"
				},
				"line-height": {
					"tiny": 1.15,
					"small": 1.2,
					"medium": 1.4,
					"normal": 1.6
				}
			}
		},
