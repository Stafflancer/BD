				},
				{
					"slug": "mulberry-to-teal",
					"gradient": "linear-gradient(143deg, #be4f81 0%, #146c94 100%)",
					"name": "Mulberry to Teal"
				},
				{
					"slug": "green-to-blue",
					"gradient": "linear-gradient(151deg, #52dc24 0%, #2271cf 100%)",
					"name": "Green to Blue"
				},
				{
					"slug": "sky-blue-to-purple",
					"gradient": "linear-gradient(112deg, #63cee6 0%, #7c42d1 100%)",
					"name": "Sky Blue to Purple"
				},
				{
					"slug": "aqua-to-violet",
					"gradient": "linear-gradient(112deg, #00fbe8 0%, #a100ff 100%)",
					"name": "Aqua to Violet"
				},
				{
					"slug": "orange-to-yellow",
					"gradient": "linear-gradient(112deg, #e74733 0%, #f2872a 100%)",
					"name": "Orange to Yellow"
				},
				{
					"slug": "sky-to-pink-to-orange-to-yellow",
					"gradient": "linear-gradient(135deg, #24c3fd, #fc0d7a 50%, #fc8d19 100%)",
					"name": "Sky to Pink to Orange to Yellow"
				},
				{
					"slug": "orange-to-pink-to-yellow",
					"gradient": "linear-gradient(135deg, #FC7417, #E32A38 50%, #F8AC24 100%)",
					"name": "Orange to Pink to Yellow"
				},
				{
					"slug": "sky-to-orange",
					"gradient": "linear-gradient(135deg, #24C3FD 0%, #FC4C14 100%)",
					"name": "Sky to Orange"
				}
			]
		},
		"layout": {
			"contentSize": "1080px",
			"wideSize": "1320px"
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "clamp(1.5rem, 5vw, 2rem)",
					"slug": "30",
					"name": "1"
				},
				{
					"size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
					"slug": "40",
					"name": "2"
				},
				{
					"size": "clamp(2.5rem, 8vw, 4.5rem)",
					"slug": "50",
					"name": "3"
				},
				{
					"size": "clamp(3.75rem, 10vw, 7rem)",
					"slug": "60",
					"name": "4"
				},
				{
					"size": "clamp(5rem, 5.25rem + ((1vw - 0.48rem) * 9.096), 8rem)",
					"slug": "70",
					"name": "5"
				},
				{
					"size": "clamp(7rem, 14vw, 11rem)",
					"slug": "80",
					"name": "6"
				}
			],
			"units": [
				"%",
				"px",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": false,
			"dropCap": false,
			"fluid": true,
			"fontWeight": false,
			"fontStyle": false,
			"letterSpacing": false,
			"lineHeight": true,
			"textDecoration": false,
			"fontFamilies": [
				{
					"fontFamily": "sofia-pro, sans-serif",
					"slug": "sofia-pro",
					"name": "Sofia Pro"
				},
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"slug": "system-fonts",
					"name": "System Fonts"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.875rem",
						"max": "1rem"
					},
					"slug": "small",
					"size": "1rem",
					"name": "S"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"slug": "medium",
