	"styles": {
		"color": {
			"background": "#fff",
			"text": "#1d2327"
		},
		"elements": {
			"button": {
				"border": {
					"color": "#f9e547",
					"width": "2px"
				},
				"color": {
					"background": "#f9e547",
					"text": "#f9e547"
				},
				":hover": {
					"color": {
						"background": "#1d252d",
						"text": "#f9e547"
					}
				},
				":focus": {
					"color": {
						"background": "#1d252d",
						"text": "#f9e547"
					}
				},
				":active": {
					"color": {
						"background": "#f9e547",
						"text": "#1d252d"
					}
				},
				":visited": {
					"color": {
						"text": "#f9e547"
					}
				},
				"typography": {
					"fontWeight": "700",
					"lineHeight": "1.33"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "clamp(2.5rem, -2.7174rem + 10.8696vw, 6.25rem)",
					"lineHeight": "0.7"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "clamp(2rem, -1.6522rem + 7.6087vw, 4.625rem)",
					"lineHeight": "1"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "clamp(1.75rem, 0.7065rem + 2.1739vw, 2.5rem)",
					"fontWeight": "900",
					"lineHeight": "1.15"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "clamp(1.25rem, 0.9022rem + 0.7246vw, 1.5rem)",
					"fontWeight": "900",
					"lineHeight": "1.25"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "clamp(1.125rem, 0.9511rem + 0.3623vw, 1.25rem)",
					"fontWeight": "900",
					"lineHeight": "1.75",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "clamp(1rem, 0.8261rem + 0.3623vw, 1.125rem)",
					"fontWeight": "400",
					"lineHeight": "1.7",
					"textTransform": "uppercase"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "700"
				}
			},
			"link": {
				"color": {
					"text": "#2e77dd"
				},
				":hover": {
					"typography": {
						"fontWeight": "700",
						"textDecoration": "none"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				":active": {
					"color": {
						"text": "#004ea8"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--sofia-pro)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		}
