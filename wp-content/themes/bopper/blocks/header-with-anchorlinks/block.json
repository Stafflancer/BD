{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "acf/header-with-anchorlinks",
    "title": "Header with Anchorlinks",
    "description": "A custom Header with Anchorlinks block",
    "category": "acf",
    "usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
    "icon": "admin-links",
    "keywords": [ "header", "with" , "anchorlinks"],
    "acf": {
        "mode": "preview",
        "renderTemplate": "header-with-anchorlinks.php"
    },
    "attributes": {
        "align": {
            "type": "string",
            "default": "full"
        },
        "alignContent": {
            "type": "string",
            "default": "center left"
        }
    },
    "supports": {
        "align": [
            "wide",
            "full"
        ],
        "anchor": true,
        "alignContent": "matrix",
        "alignText": true,
        "fullHeight": true,
        "html": true,
        "jsx": true,
        "multiple": true,
        "reusable": false
    },
    "style": ["file:./header-with-anchorlinks.css"]
}