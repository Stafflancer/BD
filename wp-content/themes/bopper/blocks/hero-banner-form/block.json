{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "acf/hero-banner-form",
	"title": "Hero Banner - Form",
	"description": "A custom hero banner form block.",
	"category": "acf",
	"usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
	"icon": "admin-home",
	"keywords": [ "hero", "banner", "form" ],
	"acf": {
		"mode": "preview",
		"renderTemplate": "hero-banner-form.php"
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "full"
		},
		"alignContent": {
			"type": "string",
			"default": "center left"
		}
	},
	"supports": {
		"align": [
			"wide",
			"full"
		],
		"anchor": true,
		"alignContent": "matrix",
		"alignText": true,
		"fullHeight": true,
		"html": true,
		"jsx": true,
		"multiple": false,
		"reusable": false
	},
	"style": "file:./hero-banner-form.css"
}