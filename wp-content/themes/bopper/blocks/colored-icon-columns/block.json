{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "acf/colored-icon-columns",
    "title": "Colored Icon Columns",
    "description": "A custom Colored Icon Columns block",
    "category": "acf",
    "usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
    "icon": "columns",
    "keywords": [ "colored", "icon", "columns" ],
    "acf": {
        "mode": "preview",
        "renderTemplate": "colored-icon-columns.php"
    },
    "attributes": {
        "align": {
            "type": "string",
            "default": "full"
        },
        "alignContent": {
            "type": "string",
            "default": "center left"
        }
    },
    "supports": {
        "align": [
            "wide",
            "full"
        ],
        "anchor": true,
        "alignContent": "matrix",
        "alignText": true,
        "fullHeight": true,
        "html": true,
        "jsx": true,
        "multiple": true,
        "reusable": false
    },
    "style": "file:./colored-icon-columns.css"
}