{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "acf/single-testimonial",
	"title": "Single Testimonial",
	"description": "A custom Single Testimonial block",
	"category": "acf",
	"usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
	"icon": "testimonial",
	"keywords": [ "single", "testimonial"],
	"acf": {
		"mode": "preview",
		"renderTemplate": "single-testimonial.php"
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "full"
		},
		"alignContent": {
			"type": "string",
			"default": "center left"
		}
	},
	"supports": {
		"align": [
			"wide",
			"full"
		],
		"anchor": true,
		"alignContent": false,
		"alignText": true,
		"fullHeight": true,
		"html": true,
		"jsx": true,
		"multiple": true,
		"reusable": true
	},
	"style": "file:./single-testimonial.css"
}