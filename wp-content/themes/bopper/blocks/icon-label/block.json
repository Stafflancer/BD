{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "acf/icon-label",
    "title": "Icon + Label",
    "description": "A custom Icon + Label block",
    "category": "acf",
    "usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
    "icon": "forms",
    "keywords": [ "icon", "label"],
    "acf": {
        "mode": "preview",
        "renderTemplate": "icon-label.php"
    },
    "attributes": {
        "align": {
            "type": "string",
            "default": "full"
        },
        "alignContent": {
            "type": "string",
            "default": "center left"
        }
    },
    "supports": {
        "align": [
            "wide",
            "full"
        ],
        "anchor": true,
        "alignContent": "matrix",
        "alignText": true,
        "fullHeight": true,
        "html": true,
        "jsx": true,
        "multiple": true,
        "reusable": false
    },
    "style": ["file:./icon-label.css"]
}