{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "acf/media-object-cards",
    "title": "Media Object Cards",
    "description": "A custom Media Object Cards block",
    "category": "acf",
    "usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
    "icon": "admin-media",
    "keywords": [ "media", "object", "cards" ],
    "acf": {
        "mode": "preview",
        "renderTemplate": "media-object-cards.php"
    },
    "attributes": {
        "align": {
            "type": "string",
            "default": "full"
        },
        "alignContent": {
            "type": "string",
            "default": "center left"
        }
    },
    "supports": {
        "align": [
            "wide",
            "full"
        ],
        "anchor": true,
        "alignContent": "matrix",
        "alignText": true,
        "fullHeight": true,
        "html": true,
        "jsx": true,
        "multiple": true,
        "reusable": false
    },
    "style": ["file:./media-object-cards.css"]
}