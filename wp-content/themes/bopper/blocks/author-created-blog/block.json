{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "acf/author-created-blog",
    "title": "Author Created Blog",
    "description": "A custom Author Created Blog block",
    "category": "acf",
    "usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
    "icon": "welcome-write-blog",
    "keywords": [ "author", "blog" ],
    "acf": {
        "mode": "preview",
        "renderTemplate": "author-created-blog.php"
    },
    "attributes": {
        "align": {
            "type": "string",
            "default": "full"
        },
        "alignContent": {
            "type": "string",
            "default": "center left"
        }
    },
    "supports": {
        "align": [
            "wide",
            "full"
        ],
        "anchor": true,
        "alignContent": "matrix",
        "alignText": true,
        "fullHeight": true,
        "html": true,
        "jsx": true,
        "multiple": true,
        "reusable": false
    },
    "style": ["file:./author-created-blog.css"]
}