{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "acf/clutch-widget",
    "title": "Clutch widget",
    "description": "A custom Clutch widget block",
    "category": "acf",
    "usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
    "icon": "admin-site-alt3",
    "keywords": [ "clutch", "widget" ],
    "acf": {
        "mode": "preview",
        "renderTemplate": "clutch-widget.php"
    },
    "attributes": {
        "align": {
            "type": "string",
            "default": "full"
        },
        "alignContent": {
            "type": "string",
            "default": "center left"
        }
    },
    "supports": {
        "align": [
            "wide",
            "full"
        ],
        "anchor": true,
        "alignContent": "matrix",
        "alignText": true,
        "fullHeight": true,
        "html": true,
        "jsx": true,
        "multiple": true,
        "reusable": false
    },
    "style": "file:./clutch-widget.css",
    "script": [ "clutch-widget-script"]
}