{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "acf/featured-resources",
    "title": "Featured Resources",
    "description": "A custom Featured Resources block",
    "category": "acf",
    "usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
    "icon": "editor-indent",
    "keywords": [ "featured", "resources" ],
    "acf": {
        "mode": "preview",
        "renderTemplate": "featured-resources.php"
    },
    "attributes": {
        "align": {
            "type": "string",
            "default": "full"
        },
        "alignContent": {
            "type": "string",
            "default": "center left"
        }
    },
    "supports": {
        "align": [
            "wide",
            "full"
        ],
        "anchor": true,
        "alignContent": "matrix",
        "alignText": true,
        "fullHeight": true,
        "html": true,
        "jsx": true,
        "multiple": true,
        "reusable": false
    },
    "style": ["swiperjs-style", "file:./featured-resources.css"],
    "script": [ "swiperjs-script", "featured-resources-script"]
}