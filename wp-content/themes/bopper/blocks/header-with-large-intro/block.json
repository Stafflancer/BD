{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "acf/header-with-large-intro",
    "title": "Header with Large Intro",
    "description": "A custom Header with Large Intro block",
    "category": "acf",
    "usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
    "icon": "media-default",
    "keywords": [ "header", "large", "intro" ],
    "acf": {
        "mode": "preview",
        "renderTemplate": "header-with-large-intro.php"
    },
    "attributes": {
        "align": {
            "type": "string",
            "default": "full"
        },
        "alignContent": {
            "type": "string",
            "default": "center left"
        }
    },
    "supports": {
        "align": [
            "wide",
            "full"
        ],
        "anchor": true,
        "alignContent": "matrix",
        "alignText": true,
        "fullHeight": true,
        "html": true,
        "jsx": true,
        "multiple": true,
        "reusable": false
    },
    "style": "file:./header-with-large-intro.css"
}